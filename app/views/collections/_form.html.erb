<%= simple_form_for(collection) do |f| %>
  <%= f.input :title,
              placeholder: 'Collection Name',
              required: true,
              minlength: 1 %>
  <%= f.input :photo, as: :file %>
  <%= hidden_field_tag 'bottle', @bottle %>
  <%= f.button :submit, class: "btn btn-warning" %>
<% end %>
